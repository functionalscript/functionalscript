{
  "name": "@functionalscript/effects",
  "version": "1.0.0",
  "description": "Pure functional effects system for FunctionalScript",
  "type": "module",
  "main": "./effects/module.f.ts",
  "exports": {
    ".": "./effects/module.f.ts",
    "./effects": "./effects/module.f.ts",
    "./effects/node": "./effects/node/module.f.ts",
    "./string": "./string/module.f.ts",
    "./array": "./array/module.f.ts",
    "./path": "./path/module.f.ts"
  },
  "scripts": {
    "test": "tsx effects/test.f.ts && tsx effects/node/test.f.ts && tsx fjs-eff/test.f.ts",
    "test:effects": "tsx effects/test.f.ts",
    "test:node": "tsx effects/node/test.f.ts",
    "test:fjs": "tsx fjs-eff/test.f.ts",
    "test:all": "npm run test:effects && npm run test:node && npm run test:fjs",
    "fjs": "tsx fjs-eff/module.ts"
  },
  "keywords": [
    "functional-programming",
    "effects",
    "io",
    "pure-functions",
    "typescript",
    "functionalscript"
  ],
  "author": "FunctionalScript Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "effects/**/*.ts",
    "string/**/*.ts",
    "array/**/*.ts",
    "path/**/*.ts",
    "fjs-eff/**/*.ts",
    "examples/**/*.ts",
    "README.md",
    "MIGRATION.md"
  ]
}
