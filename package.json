{
  "name": "functionalscript",
  "version": "0.6.9",
  "type": "module",
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "**/*.ts"
  ],
  "description": "FunctionalScript is a functional subset of JavaScript",
  "scripts": {
    "tsc-emit": "tsc --NoEmit false",
    "n": "node --trace-uncaught",
    "prepack": "npm run tsc-emit",
    "git-clean": "git clean -xf",
    "test20": "npm run tsc-emit && npm run n ./bin/fst.js",
    "test22": "tsc && npm run n -- --experimental-strip-types ./bin/fst.js",
    "test": "tsc && npm run n ./bin/fst.js",
    "index": "npm run n ./bin/index.js",
    "fsc": "npm run n ./bin/fsc.js",
    "update": "npm run index && npm install"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "fsc": "fsc/module.js",
    "fst": "dev/test/module.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/functionalscript/functionalscript.git"
  },
  "author": "Sergey Shandar",
  "license": "AGPL-3.0-only",
  "keywords": [
    "lambda",
    "functional-programming",
    "closure",
    "pure-functional",
    "typescript",
    "programming-language",
    "lazy-evaluation"
  ],
  "bugs": {
    "url": "https://github.com/functionalscript/functionalscript/issues"
  },
  "homepage": "https://github.com/functionalscript/functionalscript#readme",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "typescript": "^5.8.3"
  }
}
